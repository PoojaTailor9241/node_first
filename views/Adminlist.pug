doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous")

        title All Admins
    body.bg-secondary
        center 
            a.text-warning(href="/admin/add") Add new Member 
            br
            div.container
                table.table.table-dark.table-striped.shadow-lg.rounded
                    tr
                        th Email 
                        th Password
                        th Delete
                    br
                    if data == true 
                        for ad in AdminList 
                            tr 
                                td= ad.email
                                td= ad.password
                                td 
                                    a.btn.btn-danger(href=`/admin/delete/${ad._id}`) Delete 